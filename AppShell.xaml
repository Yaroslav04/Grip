<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Grip.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Grip"
    xmlns:view="clr-namespace:Grip.Core.View"
    Shell.FlyoutBehavior="Flyout">
    
    <ShellContent
        Title="Execute"
        ContentTemplate="{DataTemplate view:ExecutePage}"
        Route="ExecutePage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    

</Shell>
