<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Grip.MainPage">

    <StackLayout Spacing="15" Padding="5,5,5,5">
        <Button HeightRequest="50" Padding="2,2,2,2" x:Name="runServise" Text="RunServise" Clicked="runServise_Clicked"></Button>
        <Button HeightRequest="50" Padding="2,2,2,2" x:Name="stopServise" Text="StopServise" Clicked="stopServise_Clicked"></Button>
        <Button HeightRequest="50" Padding="2,2,2,2" x:Name="addTask" Text="AddTask" Clicked="addTask_Clicked"></Button>
        <Button HeightRequest="50" Padding="2,2,2,2" x:Name="addPeriod" Text="AddPeriod" Clicked="addPeriod_Clicked"></Button>
    </StackLayout>

</ContentPage>
